/* client/pages/jiance/jiance.wxss */
.bg{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
  top:-30px;
}
page{
  height: 100%;
  width: 100%;
  background-color: #313131;
}
.img1{
  object-fit: cover;
}
.img2{
  float: left;
  max-width: 41%;
  max-height: 28%;
  transform: translate(-30px,-20px);
 
}
.btn{
  width: 30%;
  background-color: #ffe169;
  display: inline-block;
  margin: 0 20px;
  text-align: center;
  transform: translate(30px);
  z-index: 2;
}
.show{
  display: none;
}
@import "../../libs/wxss/weui.wxss";
.image_viwe_size{
  display:flex;
  justify-content: center;
  background: #313131;
  margin:83px 0; 
}
.pred{
  border: none;
}
.predv{
  overflow: hidden;
  margin: 30px 30px 0 0;
  padding: 10px;
  background-color: #ffe169;
  border-radius: 5px;
}
